<div class="container1">
  <div class="row1">
    <div class="col1">
    <%= render 'maps/maps', posts: @posts %>
    </div>
  </div>
  <div class="row2" id="wrapper">
    <div class="col2">
      <% @posts.each do |posts| %>
      <div class="box" id="makeImg">
      <%= render 'videos/video', posts: posts %>
      <div class="likebuttons" id="like_buttons_<%= posts.id %>">
      <%= render 'likes/likes', posts: posts %>
      </div>
      </div>
      <% end %>
    </div>
  </div>

</div>